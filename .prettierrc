{
     "arrowParens": "always",
     "bracketSpacing": true,
     "htmlWhitespaceSensitivity": "css",
     "insertPragma": false,
     "jsxBracketSameLine": false,
     "jsxSingleQuote": false,
     "printWidth": 80,
     "proseWrap": "preserve",
     "quoteProps": "as-needed",
     "requirePragma": false,
     "semi": true,
     "singleQuote": false,
     "tabWidth": 5,
     "trailingComma": "all",
     "useTabs": false,
     "vueIndentScriptAndStyle": false,
     "editor.insertSpaces": false,
     "editor.tabSize": 4,
     "editor.detectIndentation": false,

     "prettier.tabWidth": 4,
     "prettier.useTabs": true
}
